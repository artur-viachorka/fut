{
  "manifest_version": 2,

  "name": "Fut",
  "description": "Some Desc.",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "*://*.ea.com/*"
  ],
  "browser_action": {
		"default_icon": "assets/logo.png"
  },
  "web_accessible_resources": [
    "futWebApp.bundle.js"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.ea.com/*"],
      "run_at": "document_end",
      "js": ["content.bundle.js"]
    }
  ]
}